{
  "name": "brauch-ich-ein-ga",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "storybook": "concurrently \"npm run dev:css\" \"start-storybook -p 7007\"",
    "build-storybook": "build-storybook",
    "dev:css": "npm run generate:css -- --watch",
    "generate:css": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer",
    "build:tailwind": "npm run generate:css && tailwind-rn"
  },
  "dependencies": {
    "@expo/webpack-config": "~0.16.2",
    "expo": "~45.0.0",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "0.17.7",
    "tailwind-rn": "^4.2.0",
    "webpack-dev-server": "~3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.1",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.3.5"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "private": true
}
