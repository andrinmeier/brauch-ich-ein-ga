{
  "name": "brauch-ich-ein-ga",
  "version": "1.0.0",
  "scripts": {
    "start": "run-p start:*",
    "start:expo": "expo start --dev-client",
    "start:css": "npm run generate:css -- --watch",
    "start:css-rn": "npm run generate:css-rn -- --watch",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npm run generate:css && npm run generate:css-rn && expo start --web",
    "dev": "run-p dev:*",
    "dev:web": "expo start --web",
    "dev:css": "npm run generate:css -- --watch",
    "dev:css-rn": "npm run generate:css-rn -- --watch",
    "eject": "expo eject",
    "generate:css": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer",
    "generate:css-rn": "tailwind-rn",
    "test:e2e:run": "start-server-and-test web http://localhost:19006 \"npx cypress run\""
  },
  "dependencies": {
    "@expo/webpack-config": "~0.16.2",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "date-fns": "^2.28.0",
    "dinero.js": "^2.0.0-alpha.8",
    "expo": "~45.0.0",
    "expo-dev-client": "~0.9.6",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-mmkv": "^2.4.1",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-web": "0.17.7",
    "tailwind-rn": "^4.2.0",
    "uuid": "^3.4.0",
    "webpack-dev-server": "~3.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@cypress/code-coverage": "^3.10.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "@types/uuid": "^8.3.4",
    "babel-loader": "^8.2.5",
    "babel-plugin-istanbul": "^6.1.1",
    "concurrently": "^7.2.1",
    "cypress": "^9.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.3.5"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__mocks__/globalMock.js"
    ]
  },
  "private": true
}
